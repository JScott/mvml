#!/bin/bash
COMMAND="aws s3 sync . s3://dev.mvml.net/"
FILE_ARGS=(
  "exclude '*'"
  "include '*.html'"
  "include '*.mvml'"
  "exclude 'node_modules/*'"
  "include 'js/*'"
  "include 'css/*'"
  "include 'img/*'"
  "include 'dist/*'"
  "include 'models/*'"
);
for arg in "${FILE_ARGS[@]}"
do
  COMMAND="${COMMAND} --${arg}"
done
COMMAND="${COMMAND} --acl public-read --delete"

echo ${COMMAND}
eval ${COMMAND}
